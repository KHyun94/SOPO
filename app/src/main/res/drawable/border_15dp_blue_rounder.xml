<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="1dp"
        android:right="1dp"
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">

            <corners android:radius="15dp" />

            <stroke
                android:width="3dp"
                android:color="@color/COLOR_MAIN_700" />

            <solid android:color="@color/MAIN_WHITE" />


        </shape>
    </item>
</layer-list>