<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainNavGraph"
    app:startDestination="@id/input_parcel_dest">

    <fragment
        android:id="@+id/input_parcel_dest"
        android:name="com.delivery.sopo.presentation.register.view.InputParcelFragment"
        tools:layout="@layout/fragment_input_parcel">
        <action
            android:id="@+id/action_input_parcel_dest_to_select_carrier_dest"
            app:destination="@id/select_carrier_dest" />
    </fragment>

    <fragment
        android:id="@+id/select_carrier_dest"
        android:name="com.delivery.sopo.presentation.register.view.SelectCarrierFragment"
        tools:layout="@layout/fragment_select_carrier">
        <action
            android:id="@+id/action_select_carrier_dest_to_confirm_parcel_dest"
            app:destination="@id/confirm_parcel_dest" />
    </fragment>

    <fragment
        android:id="@+id/confirm_parcel_dest"
        android:name="com.delivery.sopo.presentation.register.view.ConfirmParcelFragment"
        tools:layout="@layout/fragment_confirm_parcel"/>



</navigation>